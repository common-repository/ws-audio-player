msgid "白色经典"
msgstr "Classic"

#: E:\Docments\SVN\ws-audio-player\trunk/options-wsaudioplayer.php:120
msgid "自定义CSS"
msgstr "Custom CSS"

#: E:\Docments\SVN\ws-audio-player\trunk/options-wsaudioplayer.php:135
msgid "CSS文件不能写入！"
msgstr "CSS file can't write"

